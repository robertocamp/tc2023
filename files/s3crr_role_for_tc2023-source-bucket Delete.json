{
  "Version": "2012-10-17",
  "Statement": [
      {
          "Action": [
              "s3:ListBucket",
              "s3:GetReplicationConfiguration",
              "s3:GetObjectVersionForReplication",
              "s3:GetObjectVersionAcl",
              "s3:GetObjectVersionTagging",
              "s3:GetObjectRetention",
              "s3:GetObjectLegalHold"
          ],
          "Effect": "Allow",
          "Resource": [
              "arn:aws:s3:::tc2023-source-bucket",
              "arn:aws:s3:::tc2023-source-bucket/*",
              "arn:aws:s3:::tc2023-dest-bucket",
              "arn:aws:s3:::tc2023-dest-bucket/*"
          ]
      },
      {
          "Action": [
              "s3:ReplicateObject",
              "s3:ReplicateDelete",
              "s3:ReplicateTags",
              "s3:ObjectOwnerOverrideToBucketOwner"
          ],
          "Effect": "Allow",
          "Resource": [
              "arn:aws:s3:::tc2023-source-bucket/*",
              "arn:aws:s3:::tc2023-dest-bucket/*"
          ]
      }
  ]
}